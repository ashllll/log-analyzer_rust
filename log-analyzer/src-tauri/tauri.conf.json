{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "log-analyzer",
  "version": "0.1.0",
  "identifier": "com.joeash.log-analyzer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Log Analyzer",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "plugins": {}
}
